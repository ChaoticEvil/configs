URxvt.loginShell:  true
URxvt..transparent: true
! URxvt..shading: 0 to 99 darkens, 101 to 200 lightens
URxvt.shading: 30

URxvt.cursorBlink: 1

!URxvt.font: -xos4-terminus-medium-r-.-.-18-180-.-.-.-.-iso10646-1
URxvt.font: xft:Consolas:normal:size=14
URxvt.boldFont: xft:Consolas:bold:size=14
URxvt.italicFont: xft:Consolas:italic:size=14
URxvt.boldItalicFont: xft:Consolas:bold:italic:size=14

URxvt.scrollstyle: rxvt
urxvt.scrollBar: false
! do not scroll with output
URxvt.scrollTtyOutput: false
! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt.scrollWithBuffer: true
! scroll back to the bottom on keypress
URxvt.scrollTtyKeypress: true

URxvt.secondaryScreen: 1
URxvt.secondaryScroll: 0
URxvt.secondaryWheel: 1

URxvt.perl-ext-common: default,matcher,tabbed

URxvt.background: #000000
URxvt.foreground: #ffffff

URxvt.colorUL: #86a2b0

! black
URxvt.color0  : #000000
URxvt.color8  : #000000
! red
URxvt.color1  : #CC0000
URxvt.color9  : #EF2929
! green
URxvt.color2  : #4E9A06
URxvt.color10 : #8AE234
! yellow
URxvt.color3  : #C4A000
URxvt.color11 : #FCE94F
! blue
URxvt.color4  : #3465A4
URxvt.color12 : #729FCF
! magenta
URxvt.color5  : #75507B
URxvt.color13 : #AD7FA8
! cyan
URxvt.color6  : #06989A
URxvt.color14 : #34E2E2
! white
URxvt.color7  : #D3D7CF
URxvt.color15 : #EEEEEC

xclock*update: 1
xclock*analog: false
xclock*foreground: black
xclock*geometry: 250x28+1380+0
